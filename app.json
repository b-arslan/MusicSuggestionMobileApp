{
    "expo": {
        "name": "Beatspresso",
        "slug": "Beatspresso",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/images/Beatspresso.png",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "infoPlist": {
                "LSApplicationQueriesSchemes": ["spotify", "music"]
            }
        },
        "web": {
            "bundler": "metro",
            "output": "single",
            "favicon": "./assets/images/favicon.ico"
        },
        "plugins": ["expo-router", "expo-font", "expo-web-browser"],
        "experiments": {
            "typedRoutes": true
        }
    }
}
